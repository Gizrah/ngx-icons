{
  "name": "ngx-icons",
  "version": "0.0.0",
  "description": "A collection of icon libraries to use in your angular apps",
  "main": "index.ts",
  "scripts": {
    "transpile": "ngc",
    "package": "rollup -c",
    "minify": "uglifyjs dist/bundles/ngxicons.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/ngxicons.umd.min.js",
    "build": "npm run transpile && npm run package && npm run minify"
  },
  "keywords": [
    "Angular 4",
    "icons"
  ],
  "author": "rodgc",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "2.4.2 ||^4.2.4",
    "@angular/core": "^4.2.4",
    "font-awesome": "^4.7.0"
  },
  "peerDependencies": {
    "@angular/core": "^2.4.2 || ^4.2.3",
    "@angular/common": "2.4.2 ||^4.2.4",
    "font-awesome": "^4.7.0"
  },
  "devDependencies": {
    "@angular/cli": "^1.1.2",
    "@angular/compiler": "^2.4.2 || ^4.2.3",
    "@angular/compiler-cli": "^2.4.2 || ^4.2.3",
    "@types/node": "^7.0.22",
    "es6-shim": "^0.35.3",
    "rollup": "^0.41.6",
    "server-destroy": "^1.0.1",
    "ts-node": "^3.0.4",
    "tslint": "^5.4.0",
    "typescript": "^2.3.4",
    "uglify-js": "^3.0.14",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.4.0",
    "zone.js": "^0.8.11"
  }
}
